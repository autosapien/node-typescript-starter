{
  "name": "node-typescript-starter",
  "version": "1.0.0",
  "description": "Starter Project for Node TypeScript. CLean code :-)",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
